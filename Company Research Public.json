    {
      "parameters": {
        "jsCode": "const result = $json.organic_results?.[0] || {};\n\nlet website = result.link || \"\";\nlet domain = \"\";\n\n// Extract clean domain\ntry {\n  const url = new URL(website);\n  domain = url.hostname.replace(/^www\\./, \"\");\n} catch (e) {\n  // If it's not a valid URL, attempt manual cleanup\n  domain = website\n    .replace(/^https?:\\/\\//, \"\")\n    .replace(/^www\\./, \"\")\n    .split(\"/\")[0];\n}\n\nreturn [\n  {\n    website,\n    domain,\n    title: result.title || \"\",\n    snippet: result.snippet || \"\",\n  }\n];\n"
      },
