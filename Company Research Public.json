    },
    {
      "parameters": {
        "content": "# Update Database and initial record.\n## Result will be added to an existing database with predefined column and the initial Google Sheet's status will be updated from Pending to Complete.\n",
        "height": 336,
        "width": 880,
        "color": 6
      },
      "type": "n8n-nodes-base.stickyNote",
