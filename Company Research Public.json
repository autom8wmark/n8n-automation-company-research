      "name": "Sticky Note2"
    },
    {
      "parameters": {
        "jsCode": "// This tells n8n to grab all 6 items as an array\nconst allItems = $input.all();\n\n\n// This will hold all the data for our single Airtable row\nconst finalOutput = {};\n\n// Loop through each of the 6 task outputs\nfor (const item of allItems){\n  try {\n    // 1. Get the message object from the item\n    const msg = item.json.message;\n    // 2. Parse the content string into a real object\n    const content = JSON.parse(msg.content);\n    const task = content.task;\n\n    // 3. Use the task name to assign data to a key\n    //    You MUST update the 'case' names to match your 6 tasks\n    switch (task){\n      case \"deep_summary\":\n        finalOutput.company_name = content.result.company_name;\n        if (Array.isArray(content.result.offerings)){\n          finalOutput.offerings = content.result.offerings.join('\\n');\n        }\n        break;\n      case \"product_list\":\n        finalOutput.products = content.result.join(',');\n        break;\n      case \"value_prop\":\n        finalOutput.valueprop = content.result;\n        break;\n      case \"pain_points\":\n        // We map the array of objects into a readable string\n        const painPointsArray = content.result.map(p => {\n          return `Pain Point: ${p.pain_point}\\nDescription: ${p.description}`;\n        });\n        // Join each entry with a blank line\n        finalOutput.painPoints = painPointsArray.join('\\n\\n'); \n        break;\n      case \"opportunities\":\n        const oppsArray = content.result.map(o => {\n          return `Opportunity: ${o.opportunity}\\nDescription: ${o.description}`;\n        });\n        // Join each entry with a blank line\n        finalOutput.opportunities = oppsArray.join('\\n\\n');\n        break;\n      case \"industry_classification\":\n        finalOutput.industry = content.result.join(',');\n        break;\n      default:\n        // This helps you find tasks you forgot to add\n        console.log('Unrecognized task: ${task}');\n    }\n  } catch (error){\n    // This will catch any errors if the content string isn't valid JSON\n    console.error(\"Error parsing content:\",error, item.json.message.content);\n  }\n}\n// 3. Overwrite the item's JSON with our new, clean object\nreturn[{json: finalOutput}];"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
