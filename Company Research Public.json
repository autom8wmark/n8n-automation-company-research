    {
      "parameters": {
        "jsCode": "let html = $json.data || \"\";\n\n// 1. Remove script tags\nhtml = html.replace(/<script[\\s\\S]*?<\\/script>/gi, \"\");\n\n// 2. Remove style tags\nhtml = html.replace(/<style[\\s\\S]*?<\\/style>/gi, \"\");\n\n// 3. Remove comments\nhtml = html.replace(/<!--[\\s\\S]*?-->/g, \"\");\n\n// 4. Remove base64 images\nhtml = html.replace(/data:image\\/[^;]+;base64[^\"']+/g, \"\");\n\n// 5. Remove all HTML tags\nhtml = html.replace(/<[^>]*>/g, \" \");\n\n// 6. Collapse multiple spaces/newlines\nhtml = html.replace(/\\s+/g, \" \").trim();\n\n// 7. HARD LIMIT to avoid token overflow (VERY IMPORTANT)\nconst MAX_LENGTH = 6000;  \nhtml = html.slice(0, MAX_LENGTH);\n\nreturn [{ website_text: html }];"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
